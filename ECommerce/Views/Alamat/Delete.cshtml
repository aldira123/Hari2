@model eCommerce.ViewModels.AlamatViewModel

@{
    ViewData["Title"] = "Delete";
    Layout = "_CustomerHomeLayout";
}
<div class="featured-page">
    <div class="container">
        <div class="row">
          <div class="col-md-4 col-sm-12">
            <div class="section-heading">
              <div class="line-dec"></div>
              <h1>Delete</h1>
              <h6>Are you sure you want to delete this?</h6>
            </div>
          </div>
        </div>
      </div>
    </div>
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Kecamatan)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Kecamatan)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Kelurahan)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Kelurahan)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Rt)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Rt)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Rw)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Rw)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.KodePos)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.KodePos)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Detail)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Detail)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="IdAlamat" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Back to List</a>
    </form>




</div>

